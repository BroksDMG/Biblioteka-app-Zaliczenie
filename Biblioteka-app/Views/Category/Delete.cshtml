@model Biblioteka_app.Models.CategoryModel

@{
    ViewData["Title"] = "Delete";
}
<h1 class="display-3">Usuń Autora</h1>

<h3>Czy na pewno chcesz usunąć Categorie?</h3>
<div>
    <h4>CategoryModel</h4>
    <hr />
    <dl class="row">

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Catname)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Catname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Usuń" class="btn btn-danger" />
        <a class="btn btn-dark" asp-action="Index">Wróć do listy</a>
    </form>
</div>
